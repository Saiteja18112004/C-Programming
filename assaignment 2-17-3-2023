#include<stdio.h>
#include<conio.h>
main()
{
	int a[30],i,n,min,max,loc;
	scanf("%d",&n);
   for(i=0;i<=n-1;i++)
	{
		scanf("%d",&a[i]);
	}
	 max=a[0];
	 loc=0;
	 for(i=1;i<=n-1;i++)
	 {
	 	if(a[i]>max)
	 	{
	 		max=a[i];
	 		loc=i;
		 }
	 }
	
	printf(" %d\t",max);
	min=a[0];
	loc=0;
	for(i=1;i<=n-1;i++)
	{
	 	if(a[i]<min)
	 	{
	 		min=a[i];
	 		loc=i;
		 }
	 }
	printf("%d",min);
	
	
	
}
